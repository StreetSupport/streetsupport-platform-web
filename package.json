{
  "name": "streetsupport-platform-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "prebuild": "node ./scripts/fetch-prebuild.js",
    "build": "npm run test && npm run test:e2e && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --config ./config/jest.config.cjs",
    "test:watch": "jest --config ./config/jest.config.cjs --watch",
    "test:ci": "jest --config ./config/jest.config.cjs --ci",
    "test:e2e": "playwright test --config=./config/playwright.config.ts",
    "test:e2e:headed": "playwright test --config=./config/playwright.config.ts --headed",
    "test:e2e:debug": "playwright test --config=./config/playwright.config.ts --debug",
    "fetch:locations": "node ./scripts/fetch-locations.js",
    "fetch:service-categories": "node ./scripts/fetch-service-categories.js",
    "fetch:client-groups": "node ./scripts/fetch-client-groups.js",
    "fetch:all": "npm-run-all2 fetch:locations fetch:service-categories fetch:client-groups"
  },
  "dependencies": {
    "@google-analytics/data": "^5.2.1",
    "@googlemaps/js-api-loader": "^1.16.10",
    "@sendgrid/mail": "^8.1.6",
    "@sentry/nextjs": "^10.38.0",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.1",
    "google-auth-library": "^10.5.0",
    "lucide-react": "^0.563.0",
    "mongodb": "^6.21.0",
    "next": "^15.5.12",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^4.12.0",
    "react-markdown": "^10.1.0",
    "rehype-raw": "^7.0.0",
    "zod": "^3.25.76"
  },
  "overrides": {
    "glob": "^9.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/google.maps": "^3.58.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.24",
    "babel-jest": "^30.2.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.6.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "^15.5.12",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "npm-run-all2": "^7.0.2",
    "playwright": "^1.58.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
